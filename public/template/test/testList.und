<div class="social-box">
	<div class="header">
		<div class="tools">
			<a class="btn btn-success" href="/test/new"><i class="icon-pencil"></i> 添加</a>
		</div>
	</div>
	<div class="body">
		<div class="row-fluid">
			<div class="span12">
				<table class="table table-bordered table-condensed" id="testList">
					<thead>
						<tr>
							
							<th>名称</th>
							
							<th>操作</th>
						</tr>
					</thead>
					<tbody>
						<% _.each(tests, function(test){%>
						<tr>
							
							<td><%= .name %></td>
							
							<td  data-testId=<%= test.id %> >
								<a href="/test/<%= test.id %>" class="btn btn-small">查看</a>
								<a href="/test/<%= test.id %>/edit" class="btn btn-small">修改</a>
								<button class="btn btn-small btn-delete">删除</button></td>
						</tr>
						<% }) %>
					</tbody>
				</table>
			</div>
		</div>
		<div class="row-fluid">
			<div class="span12">
				<div class="pagination"></div>
			</div>
		</div>
	</div>
</div>