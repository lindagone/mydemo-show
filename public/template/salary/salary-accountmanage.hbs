<div class="social-box social-bordered">
	<div class="header">
		<h4 class="accountName"><strong>套账管理</strong></h4>
	</div>
	<div class="body">
		<div class="list">
			<div class="row-fluid">
				<div class="span2 offset5">
					<h4>套账列表</h4>
				</div>
			</div>
			<br />
			<div class="row-fluid">
				<div class="span11">
					<div class="pull-right">
						<button class="btn btn-primary pull-right btn-small">
						<i class="icon-plus"></i>	新建套账
						</button>
					</div>
				</div>
			</div>
			<div class="row-fluid">
				<div class="span10 offset1">
					<table class="table table-hover table-bordered table-condensed">
						<thead>
							<tr class="headColor">
								<th width=40px>序号</th>
								<th>套账名称</th>
								<th>套账管理员</th>
								<th>备注</th>
								<th>操作</th>
							</tr>
						</thead>
						<tbody>
							<tr>
								<td></td>
								<td><input type="text" value="" class="input-medium"/></td>
								<td><input type="text" placeholder="TODO:多人选人框" class="input-medium"/></td>
								<td><input type="text" value="" class="input-medium"/></td>
								<td>
								<button class="btn btn-mini btn-danger">
									保存
								</button>
								<button class="btn btn-mini">
									取消
								</button></td>
							<tr>
								<td class="inCenter">1</td>
								<td>教师基本工资</td>
								<td>白雪芹，吴成恩</td>
								<td>由2位财务人员管理</td>
								<td>
								<button class="btn btn-mini edit">
									修改
								</button>
								<button class="btn btn-mini" >
									删除
								</button></td>
							</tr>
							<tr>
								<td class="inCenter">2</td>
								<td>退休教师基本工资</td>
								<td>李燕琼，杨文海，谢铮</td>
								<td>由3位财务人员共同管理</td>
								<td>
								<button class="btn btn-mini">
									修改
								</button>
								<button class="btn btn-mini">
									删除
								</button></td>
							</tr>
							<tr>
								<td class="inCenter">3</td>
								<td>临时工基本工资</td>
								<td>杨文海</td>
								<td></td>
								<td>
								<button class="btn btn-mini">
									修改
								</button>
								<button class="btn btn-mini">
									删除
								</button></td>
							</tr>
							<tr>
								<td class="inCenter">4</td>
								<td><input type="text" value="编外人员工资" class="input-medium"/></td>
								<td><input type="text" placeholder="TODO:多人选人框" class="input-medium"/></td>
								<td><input type="text" value="" class="input-medium"/></td>
								<td>
								<button class="btn btn-mini btn-danger">
									保存
								</button>
								<button class="btn btn-mini">
									取消
								</button></td>
						</tbody>
					</table>
				</div>
			</div>
		</div>
		<div class="addnew" style="display: none;">
			<form action="#" class="form-horizontal">
				<div class="control-group">
					<label class="control-label">套账名称：</label>
					<div class="controls">
						<input type="text" name="name" class="input-medium" value=""/>
					</div>
				</div>
				<div class="control-group">
					<label class="control-label">套账管理员：</label>
					<div class="controls">
						<div class="input-append date">
							<input type="text" class="input-medium" placeholder="TODO:多人选人框" />
							<span class="add-on"> <i
								class="icon-group"> </i> </span>
						</div>
					</div>
				</div>
				<div class="control-group">
					<label class="control-label">备注：</label>
					<div class="controls">
						<textarea rows="3"></textarea>
					</div>
				</div>
				<div class="form-actions">
					<button type="button" class="btn btn-primary save">
						确定
					</button>
					<button type="button" class="btn btn-danger return">
						取消
					</button>
				</div>
			</form>
		</div>
	</div>
</div>